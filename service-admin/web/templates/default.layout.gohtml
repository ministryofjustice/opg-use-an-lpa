{{ define "default" }}
  <!DOCTYPE html>
  <html lang="en" class="govuk-template govuk-template--rebranded app-html-class">
    <head>
      <meta charset="utf-8">
      <title>{{ block "title" . }}{{ end }} - UaLPA Admin</title>
      <meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover">
      <meta http-equiv="X-UA-Compatible" content="IE=edge">

      <meta name="theme-color" content="#1d70b8">
      <link rel="icon" sizes="48x48" href="/assets/rebrand/images/favicon.ico">
      <link rel="icon" sizes="any" href="/assets/rebrand/images/favicon.svg" type="image/svg+xml">
      <link rel="mask-icon" href="/assets/rebrand/images/govuk-mask-icon.svg" color="#1d70b8">
      <link rel="apple-touch-icon" sizes="180x180" href="/assets/rebrand/images/govuk-apple-touch-icon-180.png">
      <link rel="manifest" href="/assets/rebrand/manifest.json">

      <link href="/stylesheets/all.css" rel="stylesheet">
    </head>

    <body class="govuk-template__body app-body-class">
      <script>
        document.body.className += ' js-enabled' + ('noModule' in HTMLScriptElement.prototype ? ' govuk-frontend-supported' : '');
      </script>


      <a href="#main-content" class="govuk-skip-link">Skip to main content</a>

      {{ template "header" . }}

      <div class="govuk-width-container app-width-container">
        {{ block "backlink" . }}{{ end }}

        <main class="govuk-main-wrapper app-main-class" id="main-content" role="main">
          {{ block "main" . }}{{ end }}
        </main>
      </div>

      <footer class="govuk-footer" role="contentinfo"> </footer>

      <script src="/javascript/main.js"></script>
    </body>
  </html>
{{ end }}
